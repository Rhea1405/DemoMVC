
@{
    ViewBag.Title = "Index1";
}

<h2>Index1</h2>
@using(Html.BeginForm("radioextract","Radio2"))
{
<table>
    <tr>
        <td>EMPNO</td>
        <td>ENAME</td>
        <td>JOB</td>
        <td>MGR</td>
        <td>HIREDATE</td>
        <td>SAL</td>
        <td>COMM</td>
        <td>DEPTNO</td>
        <td><input type="submit" value="submit" /></td>
    </tr>

    @foreach (var i in ViewBag.emplist as IList<DemoMVC.Models.EMPDATA>)
    {
        <tr>
            <td>@Html.RadioButton("rdb", i.EMPNO)</td>
            <td>@i.EMPNO</td>
            <td>@i.ENAME</td>
            <td>@i.JOB</td>
            <td>@i.MGR</td>
            <td>@i.HIREDATE</td>
            <td>@i.SAL</td>
            <td>@i.COMM</td>
            <td>@i.DEPTNO</td>

        </tr>

    }


</table>



}


